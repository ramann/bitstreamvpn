<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.company.dev.model.app.domain.Certificate">
        <table name="certificate" schema="public" catalog="test1"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <many-to-one name="users" fetch="LAZY">
                <join-column name="username"/>
            </many-to-one>
            <basic name="dateInitiated">
                <column name="date_initiated" precision="-1"/>
            </basic>
            <basic name="csrText">
                <column name="csr_text" length="4096"/>
            </basic>
            <basic name="signed">
                <column name="signed" precision="-1"/>
            </basic>
            <basic name="certText">
                <column name="cert_text" nullable="false" length="4096"/>
            </basic>
            <basic name="revoked">
                <column name="revoked" nullable="false" precision="-1"/>
            </basic>
            <basic name="serial">
                <column name="serial" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
